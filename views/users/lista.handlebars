<table class="table "  >
    <tr>
        <td>ID</td>
        <td>Nome</td>
        <td>Email</td>
        <td colspan="3"></td>   
    </tr>
    {{#each users}}
        <tr>
            <th>{{this.id}}</td>
            <td>{{this.nome}}</td>
            <td>{{this.email}}</td>
            <td><a href="/users/delete/{{this.id}}">Excluir</a></td>
            <td><a href="/users/edit/{{this.id}}">Editar</a></td>
            <td><a href="/users/view/{{this.id}}">Visualizar</a></td>

        </tr>
    {{/each}}
</table>


